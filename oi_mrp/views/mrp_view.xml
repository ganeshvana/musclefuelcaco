<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="mrp_production_form_view_musclefuel" model="ir.ui.view">
        <field name="name">mrp.production.form.musclefuel</field>
        <field name="model">mrp.production</field>
        <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='bom_id']" position="after">
                <field name="total_carbs" readonly="1"/>
                <field name="total_proteins" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>
