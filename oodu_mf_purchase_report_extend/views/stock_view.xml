<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_form_inherit_stock" model="ir.ui.view">
        <field name="name">stock.picking.form.inherit.stock</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date_done']" position="replace">
                <field name="date_done" string="Effective Date" readonly="0"/>

            </xpath>
            
        </field>
    </record>

   
</odoo>
